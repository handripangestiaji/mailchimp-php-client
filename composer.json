{
    "name": "HandriP/mailchimp-php-client",
    "description": "Simple Mailchimp php client",
    "type": "package",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://handripangestiaji@bitbucket.org/handripangestiaji/fw-http-transport.git"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "authors": [
        {
            "name": "Handri Pangestiaji",
            "email": "handri.pangestiaji@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "HandriP\\Mailchimp\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "findwork/http-transport": "dev-master"
    },
    "autoload-dev": {
        "psr-4": {
            "HandriP\\Mailchimp\\Test\\": "tests/"
        }
    }
}
